{
	"name": "coral-command",
	"description": "Tiny generator-driven command framework for @discordjs/core and discord.js",
	"packageManager": "yarn@4.0.1",
	"type": "module",
	"version": "0.1.0",
	"author": {
		"name": "Denis Cristea",
		"email": "didinele.dev@gmail.com"
	},
	"exports": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/didinele/coral-command.git"
	},
	"bugs": {
		"url": "https://github.com/didinele/coral-command/issues"
	},
	"homepage": "https://github.com/didinele/coral-command",
	"scripts": {
		"build": "tsup && tsc",
		"prepack": "yarn build",
		"lint": "eslint src --ext .ts && prettier --check .",
		"format": "prettier --write .",
		"update": "yarn upgrade-interactive",
		"postinstall": "is-ci || husky"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.6.1",
		"@commitlint/config-angular": "^18.6.1",
		"@discordjs/rest": "^2.3.0",
		"@types/is-ci": "^3.0.4",
		"eslint": "^8.53.0",
		"eslint-config-neon": "^0.1.57",
		"husky": "^9.0.11",
		"is-ci": "^3.0.1",
		"lint-staged": "^15.0.2",
		"prettier": "^3.0.3",
		"prettier-eslint": "^16.1.2",
		"tsup": "^7.2.0",
		"turbo": "^1.10.16",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@discordjs/core": "^1.2.0",
		"@vladfrangu/async_event_emitter": "^2.4.3"
	}
}
